<div class="page-header">
  <div class="pull-right">
    <%= link_to 'New Project', new_project_path, class: "btn btn-info" %>
  </div>
  <h1>Projects</h1>
</div>

<table class="table">
  <thead>
    <tr>
      <th class="col-sm-2">Name</th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
    <tr>
      <td class="col-sm-2">
        <%= link_to "Name", project_path(project) %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>
